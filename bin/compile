#!/usr/bin/env node
"use strict";

const {join, resolve} = require("path");
const configPath      = join(resolve(__dirname, ".."), "config.cson");
const outputPath      = join(resolve(__dirname, ".."), "lib", "icons", ".icondb.js");
const IconCompiler    = require("../lib/icons/icon-compiler.js");

IconCompiler.compileConfig(configPath, outputPath);
